{
  "name": "@typhonjs-svelte/standard",
  "version": "0.0.0",
  "description": "Provides a standard Svelte component library for the TyphonJS Runtime Library.",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "imports": {
    "#runtime/*": "@typhonjs-svelte/runtime-base/*",
    "#standard/component": {
      "types": "./_dist/component/index.d.ts",
      "default": "@typhonjs-svelte/standard/component"
    },
    "#standard/*": "@typhonjs-svelte/standard/*",
    "#svelte/*": "svelte/*",
    "#svelte": "svelte"
  },
  "exports": {
    "./application": {
      "types": "./_dist/application/index.d.ts",
      "import": "./_dist/application/index.js"
    },
    "./component": {
      "types": "./_dist/component/index.d.ts",
      "import": "./_dist/component/index.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "svelte": ">=4.x.x <5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@typhonjs-build-test/esm-d-ts": "^0.3.0-next.8",
    "@typhonjs-build-test/esm-d-ts-plugin-svelte": "^0.1.0-next.1",
    "@typhonjs-build-test/rollup-plugin-pkg-imports": "^0.5.0",
    "@typhonjs-config/eslint-config": "^0.6.3",
    "@typhonjs-svelte/runtime-base": "github:typhonjs-svelte/runtime-base",
    "@typhonjs-utils/file-util": "^0.1.0",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "rollup": "^4.22.4"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/typhonjs-svelte/standard#readme",
  "keywords": [
    "typhonjs",
    "trl",
    "svelte",
    "components"
  ],
  "scripts": {
    "build": "node rollupAll.js",
    "eslint": "eslint ."
  },
  "files": [
    "/_dist"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-svelte/standard",
  "bugs": "https://github.com/typhonjs-svelte/standard/issues"
}
